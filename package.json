{
  "name": "my-music",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "main",
  "author": {
    "name": "zhaojinxing",
    "email": "1182999456@qq.com",
    "url": "https://github.com/Zjinxing"
  },
  "build": {
    "appId": "com.example.com",
    "productName": "MyMusic",
    "copyright": "Copyright Â© 2020 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "main/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "directories": {
      "buildResources": "assets"
    },
    "extends": null
  },
  "dependencies": {
    "antd": "^4.5.2",
    "axios": "^0.19.2",
    "dayjs": "^1.8.34",
    "mobx": "^6.0.1",
    "mobx-react": "^7.0.0",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=dev concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "rescripts eject",
    "postinstall": "electron-builder install-app-deps",
    "preelectron:build": "yarn build",
    "electron:build": "electron-builder -mw"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-env": "^0.0.12",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^0.9.1",
    "electron": "^15.5.5",
    "electron-builder": "^22.6.1",
    "electron-rebuild": "^1.11.0",
    "react-app-rewired": "^2.1.6",
    "wait-on": "^5.0.0"
  },
  "rescripts": [
    "env"
  ]
}